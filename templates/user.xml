<templateSet group="user">
  <template name="cd" value="/**&#10; * ClassName: $ClassName$  &lt;/br&gt;&#10; * Description:   &lt;/br&gt;&#10; * Creator: $Creator$ &lt;/br&gt;&#10; * Date: $Date$ $Time$  &lt;/br&gt;&#10; */" description="类注释" toReformat="false" toShortenFQNames="true" useStaticImport="true">
    <variable name="ClassName" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="false" />
    <variable name="Creator" expression="user()" defaultValue="" alwaysStopAt="true" />
    <variable name="Date" expression="date()" defaultValue="" alwaysStopAt="true" />
    <variable name="Time" expression="time()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
      <option name="KOTLIN" value="true" />
    </context>
  </template>
</templateSet>